{
  "name": "patroller-api",
  "version": "1.0.0",
  "description": "Patroller Services API Built for Mt Tahoma Trails Association",
  "main": "./bin/app.js",
  "scripts": {
    "prepare": "npx tsc",
    "build": "npx tsc",
    "start": "node ./bin/app.js",
