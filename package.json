    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@smartrecruiters/openapi-first": "^0.3.2",
    "@smartrecruiters/openapi-schemas-validator": "^0.2.0",
    "@types/express": "^4.16.1",
    "express": "^4.16.4",
    "openapi-schema-validation": "^0.4.2"
  }
