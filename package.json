{
  "name": "patroller-api",
  "version": "1.0.0",
  "description": "Patroller Services API Built for Mt Tahoma Trails Association",
  "main": "./bin/app.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf ./dist && npx tsc",
    "gen:spec": "cd ./node_modules/skimtta-oas && npm run gen:spec",
    "gen:tests": "npm run gen:spec && npx oatts generate -s ./node_modules/skimtta-oas/dist/openapi.json -w ./tests",
    "gen:dts": "npm run gen:spec && npx dtsgen -o ./api.d.ts ./node_modules/skimtta-oas/dist/openapi.json",
    "start": "npm run build && node ./bin/app.js",
    "calendar": "npm run build && node ./bin/calendar.js",
    "format:check": "npx prettier -l './lib/**/*.ts'",
    "format:update": "npx prettier --write './lib/**/*.ts'",
    "lint": "npx tslint --project . --config tslint.json",
    "tests": "npx mocha ./tests"
  },
  "keywords": [
    "MTTA",
    "Ski",
    "Patrol"
  ],
  "author": "Ryan Tate <ryan.michael.tate@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "chakram": "^1.5.0",
    "dtsgenerator": "^2.3.1",
    "mocha": "^6.2.2",
    "oatts": "^1.5.0",
    "prettier": "^1.19.1",
    "skimtta-oas": "file:../openapi-specification",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@smartrecruiters/openapi-first": "^0.4.2",
    "@smartrecruiters/openapi-schemas-validator": "^0.2.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/uuid": "^3.4.6",
    "argon2": "^0.24.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "generate-password": "^1.4.2",
    "googleapis": "^39.2.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^4.5.1",
    "node-google-calendar": "^1.1.1",
    "openapi-schema-validation": "^0.4.2",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "swagger-routes-express": "^3.1.1",
    "typeorm": "^0.2.25",
    "uuid": "^3.3.3"
  }
}
